{
  "name": "@ada-anvil/merkle",
  "description": "TypeScript utilities for working with Merkle trees",
  "version": "0.0.0",
  "author": "Ada Anvil",
  "bugs": {
    "url": "https://github.com/cardano-forge/anvil-merkle/issues"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/cardano-forge/anvil-merkle",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cardano-forge/anvil-merkle.git"
  },
  "keywords": [
    "javascript",
    "typescript",
    "typesafe",
    "merkle",
    "tree",
    "anvil",
    "cardano",
    "forge",
    "crypto"
  ],
  "sideEffects": false,
  "files": [
    "**"
  ],
  "main": "./core.js",
  "module": "./core.mjs",
  "types": "./core.d.mts",
  "scripts": {
    "build": "tsup",
    "check": "tsc --noEmit",
    "lint": "biome check --write .",
    "bench": "vitest bench --run",
    "test": "vitest --run",
    "test:watch": "vitest",
    "pre": "npm run lint && npm run check && npm run build && npm run test"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./core.d.ts",
        "require": "./core.js"
      },
      "import": {
        "types": "./core.d.mts",
        "import": "./core.mjs"
      }
    },
    "./crypto": {
      "require": {
        "types": "./crypto.d.ts",
        "require": "./crypto.js"
      },
      "import": {
        "types": "./crypto.d.mts",
        "import": "./crypto.mjs"
      }
    },
    "./csl": {
      "require": {
        "types": "./csl.d.ts",
        "require": "./csl.js"
      },
      "import": {
        "types": "./csl.d.mts",
        "import": "./csl.mjs"
      }
    }
  },
  "devDependencies": {
    "@biomejs/biome": "2.0.6",
    "@emurgo/cardano-serialization-lib-nodejs-gc": "^15.0.1",
    "@faker-js/faker": "^10.1.0",
    "@types/node": "^22.13.4",
    "tsup": "^8.3.6",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5"
  },
  "peerDependencies": {
    "@emurgo/cardano-serialization-lib-nodejs-gc": ">=14.0.0"
  },
  "peerDependenciesMeta": {
    "@emurgo/cardano-serialization-lib-nodejs-gc": {
      "optional": true
    }
  },
  "dependencies": {
    "trynot": "^0.0.2"
  }
}
